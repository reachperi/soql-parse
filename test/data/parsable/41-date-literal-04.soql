SELECT Id, SomeNumberField__c
FROM Account
WHERE CreadedDate = YESTERDAY
OR CreadedDate = TODAY
OR CreadedDate = TOMORROW
OR CreadedDate = LAST_WEEK
OR CreadedDate = THIS_WEEK
OR CreadedDate = NEXT_WEEK
OR CreadedDate = LAST_MONTH
OR CreadedDate = THIS_MONTH
OR CreadedDate = NEXT_MONTH
OR CreadedDate = LAST_90_DAYS
OR CreadedDate = NEXT_90_DAYS
OR CreadedDate = THIS_QUARTER
OR CreadedDate = LAST_QUARTER
OR CreadedDate = NEXT_QUARTER
OR CreadedDate = THIS_YEAR
OR CreadedDate = LAST_YEAR
OR CreadedDate = NEXT_YEAR
OR CreadedDate = THIS_FISCAL_QUARTER
OR CreadedDate = LAST_FISCAL_QUARTER
OR CreadedDate = NEXT_FISCAL_QUARTER
OR CreadedDate = THIS_FISCAL_YEAR
OR CreadedDate = LAST_FISCAL_YEAR
OR CreadedDate = NEXT_FISCAL_YEAR
OR CreadedDate = LAST_N_DAYS : 12
OR CreadedDate = NEXT_N_DAYS : 12
OR CreadedDate = NEXT_N_WEEKS : 12
OR CreadedDate = LAST_N_WEEKS : 12
OR CreadedDate = NEXT_N_MONTHS : 12
OR CreadedDate = LAST_N_MONTHS : 12
OR CreadedDate = NEXT_N_QUARTERS : 12
OR CreadedDate = LAST_N_QUARTERS : 12
OR CreadedDate = NEXT_N_YEARS : 12
OR CreadedDate = LAST_N_YEARS : 12
OR CreadedDate = NEXT_N_FISCAL_QUARTERS : 12
OR CreadedDate = LAST_N_FISCAL_QUARTERS : 12
OR CreadedDate = NEXT_N_FISCAL_YEARS : 12
OR CreadedDate = LAST_N_FISCAL_YEARS : 12